{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["bun-types", "node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
  }
}
